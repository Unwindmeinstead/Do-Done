{
  "name": "Do.Done",
  "type": "react",
  "description": "Minimalist Todo App with React and Tailwind CSS",
  "framework": "react",
  "language": "javascript",
  "mainFile": "src/App.js",
  "startCommand": "npm start",
  "buildCommand": "npm run build",
  "testCommand": "npm test",
  "port": 3000,
  "features": {
    "git": true,
    "branches": true,
    "terminal": true,
    "fileExplorer": true,
    "search": true
  },
  "git": {
    "enabled": true,
    "remote": "origin",
    "branches": ["main", "ui-testing"],
    "defaultBranch": "main"
  },
  "paths": {
    "src": "src",
    "public": "public",
    "build": "build",
    "tests": "__tests__"
  },
  "dependencies": {
    "react": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "lucide-react": "^0.263.1"
  }
}